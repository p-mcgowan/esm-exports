{
  "name": "esm-exports",
  "version": "2.1.0",
  "description": "Parse ecmascript modules and collect names of export vars, functions, etc.",
  "main": "lib",
  "typings": "src/index.ts",
  "scripts": {
    "fixtures": "npm i --no-save angular2-calendar rxjs gulp-tslint @angular/core @types/express @types/fs-extra preact type-zoo hover",
    "pretest": "npm run fixtures",
    "test": "npm run eslint && npm run tscheck && npm run test:r",
    "test:r": "npm run mocha -- src/*.spec.ts",
    "mocha": "node -r ts-node/register node_modules/mocha/bin/_mocha",
    "test:w": "npm run mocha -- --watch-extensions ts --watch src/**/*.spec.ts",
    "test:d": "node --inspect -r ts-node/register node_modules/mocha/bin/_mocha --no-timeouts src/**/*.spec.ts",
    "test:brk": "node --inspect-brk -r ts-node/register node_modules/mocha/bin/_mocha --no-timeouts src/**/*.spec.ts",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --pretty false --strictNullChecks --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals --noUnusedParameters",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint": "eslint --ext ts src",
    "eslint:fix": "eslint src --ext \"ts\" --fix",
    "eslint:w": "chokidar \"src/**/*.ts\" --initial -c \"npm run eslint\"",
    "lint:w": "concurrently -- \"npm run -s tsclint:w\" \"npm run -s eslint:w\"",
    "semantic-release": "semantic-release",
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "prebuild": "npm run clean",
    "clean": "rimraf lib",
    "commit": "git-cz",
    "benchmark": "node -r ts-node/register node_modules/benchmarko/lib/bin.js -f benchmarks/parse-module-node"
  },
  "dependencies": {
    "object-values": "^1.0.0",
    "resolve": "^1.5.0",
    "tslib": "^1.9.0",
    "typescript": "^2.7.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^2.0.1",
    "@semantic-release/git": "^4.0.1",
    "@semantic-release/github": "^4.2.6",
    "@semantic-release/npm": "^3.2.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "@types/resolve": "0.0.7",
    "benchmarko": "0.0.2",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^3.5.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.18.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-tslint": "^2.1.0",
    "eslint-plugin-typescript": "^0.9.0",
    "eslint-plugin-unicorn": "^4.0.2",
    "mocha": "^5.0.4",
    "pkg-dir": "^2.0.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.0.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript-eslint-parser": "^14.0.0"
  },
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm"
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "success": [
      "@semantic-release/github"
    ],
    "fail": [
      "@semantic-release/github"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/esm-exports.git"
  },
  "keywords": [
    "esm",
    "typescript",
    "parser",
    "es2015",
    "exports"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unlight/esm-exports/issues"
  },
  "homepage": "https://github.com/unlight/esm-exports#readme"
}
