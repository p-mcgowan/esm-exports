{
  "name": "esm-exports",
  "version": "0.8.3",
  "description": "Parse ecmascript modules and collect names of export vars, functions, etc.",
  "main": "lib",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run compile",
    "compile": "tsc",
    "prebuild": "npm run clean",
    "mocha": "ts-node -F node_modules/mocha/bin/_mocha",
    "debug": "inspect node_modules/ts-node/dist/_bin.js -F \"node_modules/mocha/bin/_mocha\" --no-timeouts \"src/*.spec.ts\"",
    "test": "npm run t",
    "test:w": "npm run mocha -- --watch-extensions ts --watch src/**/*.spec.ts",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "npm run tsc -- --noEmit --pretty false --strictNullChecks --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals",
    "tsclint:w": "npm run tsclint -- --watch",
    "release": "npm run build && np"
  },
  "devDependencies": {
    "@angular/core": "^4.4.4",
    "@types/benchmark": "^1.0.30",
    "@types/lodash": "^4.14.76",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.31",
    "@types/through2": "~2.0.33",
    "angular2-calendar": "^0.3.7",
    "benchmark": "^2.1.4",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^3.5.0",
    "gulp-tslint": "^8.1.2",
    "mocha": "^3.5.3",
    "pkg-dir": "~2.0.0",
    "rxjs": "^5.4.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0"
  },
  "dependencies": {
    "@iamthes/inject": "^1.0.1",
    "lodash": "^4.17.4",
    "resolve-pkg": "^1.0.0",
    "typescript": "2.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unlight/esm-exports.git"
  },
  "license": "Xnet",
  "bugs": {
    "url": "https://github.com/unlight/esm-exports/issues"
  },
  "homepage": "https://github.com/unlight/esm-exports#readme"
}
