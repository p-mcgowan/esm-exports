{
  "name": "esm-exports",
  "version": "0.3.2",
  "description": "Parse ecmascript modules and collect names of export vars, functions, etc.",
  "main": "lib",
  "typings": "lib/index.d.ts",
  "scripts": {
    "dev": "npm run compile && concurrently -r -p \"{name}:\" -n \"w,t\" \"npm run compile:w\" \"npm run test:w\"",
    "compile": "tsc",
    "compile:w": "tsc --watch",
    "test:w": "mocha --watch lib/*.test.js",
    "test:r": "mocha lib/*.test.js",
    "test": "npm run compile && npm run test:r"
  },
  "devDependencies": {
    "@angular/core": "~2.4.2",
    "@types/lodash": "~4.14.48",
    "@types/mocha": "^2.2.39",
    "@types/node": "~6.0.59",
    "@types/through2": "~2.0.32",
    "angular2-calendar": "~0.3.6",
    "concurrently": "3.3.0",
    "gulp-tslint": "~7.0.1",
    "mocha": "^3.2.0",
    "mock-require": "~1.3.0",
    "node-resolve": "^1.3.3",
    "pkg-dir": "~1.0.0",
    "rxjs": "~5.0.3",
    "tslint": "~4.3.1"
  },
  "dependencies": {
    "fs-readfile-promise": "~3.0.0",
    "is-relative-path": "~1.0.1",
    "lodash": "^4.17.4",
    "recursive-readdir": "~2.1.0",
    "resolve-pkg": "~0.2.0",
    "typescript": "~2.1.6",
    "unixify": "~0.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unlight/esm-exports.git"
  },
  "license": "Xnet",
  "bugs": {
    "url": "https://github.com/unlight/esm-exports/issues"
  },
  "homepage": "https://github.com/unlight/esm-exports#readme"
}
