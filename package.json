{
  "name": "esm-exports",
  "version": "0.8.4",
  "description": "Parse ecmascript modules and collect names of export vars, functions, etc.",
  "main": "lib",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc",
    "mocha": "ts-node -F node_modules/mocha/bin/_mocha",
    "t": "npm run mocha -- src/*.spec.ts",
    "test": "npm run t",
    "test:w": "npm run mocha -- --watch src/**/*.spec.ts --watch-extensions ts",
    "prerelease": "npm run clean",
    "release": "npm run build",
    "inspect": "inspect node_modules/ts-node/dist/_bin.js -F node_modules/mocha/bin/_mocha --no-timeouts src/**/*.spec.ts",
    "prepublish": "npm run install:playground && npm run release",
    "install:playground": "npm i @angular/core @nestjs/common @types/lodash @types/through2 angular2-calendar chokidar-cli gulp-tslint pkg-dir rxjs tslint"
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.30",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.47",
    "benchmark": "^2.1.4",
    "concurrently": "^3.5.0",
    "mocha": "^4.0.1",
    "ts-node": "^3.3.0"
  },
  "dependencies": {
    "njct": "^1.0.0",
    "lodash": "^4.17.4",
    "resolve-pkg": "^1.0.0",
    "typescript": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unlight/esm-exports.git"
  },
  "license": "Xnet",
  "bugs": {
    "url": "https://github.com/unlight/esm-exports/issues"
  },
  "homepage": "https://github.com/unlight/esm-exports#readme"
}
