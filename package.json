{
  "name": "esm-exports",
  "version": "1.1.0-alpha.4",
  "description": "Parse ecmascript modules and collect names of export vars, functions, etc.",
  "main": "lib",
  "typings": "src/index.ts",
  "scripts": {
    "fixtures": "npm i --no-save angular2-calendar rxjs gulp-tslint @angular/core",
    "pretest": "npm run fixtures",
    "test": "npm run eslint && npm run tscheck && npm run t",
    "t": "npm run mocha -- src/*.spec.ts",
    "test:r": "npm run mocha -- src/*.spec.ts",
    "mocha": "ts-node -F node_modules/mocha/bin/_mocha",
    "test:w": "npm run mocha -- --watch-extensions ts --watch src/**/*.spec.ts",
    "test:d": "node --inspect-brk node_modules/ts-node/dist/_bin.js -F node_modules/mocha/bin/_mocha --no-timeouts src/**/*.spec.ts",
    "tscheck": "echo tscheck... && tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "tsclint": "tsc --noEmit --pretty false --strictNullChecks --forceConsistentCasingInFileNames --noImplicitReturns --noImplicitThis --noUnusedLocals --noUnusedParameters",
    "tsclint:w": "npm run tsclint -- --watch",
    "eslint": "eslint --ext ts src",
    "eslint:fix": "eslint src --ext \"ts\" --fix",
    "eslint:w": "chokidar \"src/**/*.ts\" --initial -c \"npm run eslint\"",
    "lint:w": "concurrently -- \"npm run -s tsclint:w\" \"npm run -s eslint:w\"",
    "build": "tsc",
    "clean": "rimraf lib",
    "prerelease": "npm run clean",
    "release": "standard-version",
    "prepublishOnly": "npm run build",
    "setup": "npm i -g commitizen standard-version && commitizen init cz-conventional-changelog --save-dev",
    "commit": "git-cz"
  },
  "dependencies": {
    "object-values": "^1.0.0",
    "resolve": "^1.5.0",
    "tslib": "^1.8.0",
    "typescript": "^2.6.1"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.51",
    "@types/resolve": "0.0.4",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^3.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.11.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-tslint": "^1.0.2",
    "eslint-plugin-typescript": "^0.8.0",
    "eslint-plugin-unicorn": "^3.0.0",
    "mocha": "^4.0.1",
    "pkg-dir": "^2.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "typescript-eslint-parser": "^9.0.0"
  },
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/typescript-exports.git"
  },
  "keywords": [
    "esm",
    "typescript",
    "parser",
    "es2015",
    "exports"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unlight/typescript-exports/issues"
  },
  "homepage": "https://github.com/unlight/typescript-exports#readme"
}
