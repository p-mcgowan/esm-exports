{
  "name": "esm-exports",
  "version": "0.7.0",
  "description": "Parse ecmascript modules and collect names of export vars, functions, etc.",
  "main": "lib",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "tsc",
    "compile:w": "tsc --watch",
    "mocha": "ts-node -F node_modules/mocha/bin/_mocha",
    "t": "npm run mocha -- src/*.spec.ts",
    "test": "npm run t",
    "test:w": "npm run mocha -- --watch src/**/*.spec.ts --watch-extensions ts",
    "release": "npm run clean && npm run compile",
    "inspecttest": "inspect node_modules/ts-node/dist/_bin.js -F node_modules/mocha/bin/_mocha src/**/*.spec.ts"
  },
  "devDependencies": {
    "@angular/core": "^2.4.10",
    "@types/lodash": "^4.14.56",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.8",
    "@types/through2": "~2.0.32",
    "angular2-calendar": "~0.3.6",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^3.4.0",
    "gulp-tslint": "^7.1.0",
    "mocha": "^3.2.0",
    "pkg-dir": "~1.0.0",
    "rxjs": "^5.2.0",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.1"
  },
  "dependencies": {
    "@iamthes/inject": "^1.0.0",
    "is-relative-path": "~1.0.1",
    "lodash": "^4.17.4",
    "resolve-pkg": "^1.0.0",
    "typescript": "^2.2.1",
    "unixify": "~0.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unlight/esm-exports.git"
  },
  "license": "Xnet",
  "bugs": {
    "url": "https://github.com/unlight/esm-exports/issues"
  },
  "homepage": "https://github.com/unlight/esm-exports#readme"
}
